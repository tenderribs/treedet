<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<arg name="rviz_config_file" default="$(find treedet_ros)/rviz/view_dets.rviz" />

	<!-- Launch tree_detection node -->
	<node name="inference.py" pkg="treedet_ros" type="inference.py" output="screen" />
	<node name="rviz_tree_detection" pkg="rviz" type="rviz" args="-d $(arg rviz_config_file)" required="true"/>
</launch>