<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<arg name="gui" default="true" />
	<arg name="rviz_config_file" default="$(find treedet_ros)/rviz/select_targets.rviz" />
	<arg name="use_sim_time" default="true"/>

	<param name="/use_sim_time" value="$(arg use_sim_time)"/>

	<node pkg="treedet_ros" name="treedet_ros" type="select_targets" output="screen" />
	<node if="$(arg gui)" name="rviz_tree_detection" pkg="rviz" type="rviz" args="-d $(arg rviz_config_file)" required="true"/>
</launch>